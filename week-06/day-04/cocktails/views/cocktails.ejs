      <%  })   %>
    </ul>
  </div>
  <div>
    <ul>
      <% cocktails.forEach(elem => { %>
      <li>
        <%= elem['name'] %>
      </li>
      <%  }) 
    %>

    </ul>
  </div>

</body>

</html>